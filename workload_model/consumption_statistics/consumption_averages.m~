function [pTotal, priceTotal] = consumption_averages(memUsage, nCpusUsed, timeElapsed, country)

pot_cte = 153.4;
mem_cte = 0.56;
cpu_cte = 0.85;
n_cpu_base = 12;
priceTotal = 0;

mem_consumption = mem_cte*memUsage;
cpu_consumption = cpu_cte*nCpusUsed*100/n_cpu_base;
pTotal = pot_cte + mem_consumption + cpu_consumption;

posibilities = {'sp','spain','1';
                'uk','united kingdom','2';
                'us','united states', '3';
                'cmp','compare','4'};
choose_country = strcmp(country, posibilities);

sw = 0;
for i = 1:4
    for j = 1:3
        if choose_country(i,j) == 1
            sw = i;
        end;
    end;
end;

switch sw
    case 1
        disp('spain');
        cd ~/tfg/SmartGrid/energy_pricing/comparing
        load('sp_average_day.mat');
        cd ~/tfg/SmartGrid/workload_model/plots
        load('diaOptimized.mat');
        
    case 2
        disp('uk');
    case 3
        disp('us');
    case 4
        disp('compare');
    otherwise
        disp('bad parameter');
end